/**index.wxss**/
@import "../images/common.wxss";
.userBox{
  width: 100%;
  height: 160rpx;
  border-top: 1px #d2d2d2 solid;
  border-bottom: 1px #d2d2d2 solid;
  line-height: 60rpx;
  position: relative;
}
.userPhoto{
  width: 120rpx; 
  height: 120rpx;
  top: 20rpx;
  left: 30rpx;
  background-color: #ddccff;
  position: absolute;
  border-radius:2rex;
}
.userName{
  width: 184rpx; 
  height: 60rpx;
  top: 50rpx;
  left: 200rpx;
  position: absolute;
}
.userTel{
  width: 284rpx; 
  height: 60rpx;
  bottom: 20rpx;
  left: 200rpx;
  position: absolute;
}
.userArrow{
  width: 284rpx; 
  height: 60rpx;
  top: 50rpx;
  right: 30rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  position: absolute;
}
.cutLine{
  width:100%;
  height:24rpx;
  background-color: #f2f2f2;
  border-bottom: 1px #d2d2d2 solid;
}
switch{margin-right: 24rpx}

/*隐藏元素*/
.hide{
    display:none;    
}
.show{
  display:block;
}
.notOnline{
  font-size:24rpx; text-align: right; margin-right:10rpx;
}

