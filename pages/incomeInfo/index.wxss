/**index.wxss**/
@import "../images/common.wxss";
.topBox{
  width:100%;
  height: 230rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-top: 1rpx #d2d2d2 solid;
  background-color: #fff;
}
/*内部元素 单行，横排，上下左右居中*/
.centerA{
  display: flex;
  justify-content: center;
  align-items: center;
}
/*内部元素 单行，横排，上下左右居中*/
.centerB{
  display: flex;
  flex-direction: row;
  
}

.titleTxt{
  width:100%;
  height: 96rpx;  
}
.btnBox{
  width:360rpx;
  height:74rpx;
  border:2rpx #3DCAE6 solid;
  border-radius: 39rpx;
  margin-bottom: 35rpx;
  overflow: hidden;
}
.btnLeft{
  width:183rpx;
  height: 66rpx;  
}
.btnRight{
  width:180rpx;
  height: 66rpx;
  background-color: #fff;
}
.backColor{
  background-color: #3DCAE6;
}
.cutLine{
  margin-top: 20rpx;
  width:100%;
  height:24rpx;
  background-color: #f2f2f2;
  border-top: 1rpx #d2d2d2 solid;
  border-bottom: 1rpx #d2d2d2 solid;
}
.cutListBox{
  width: 100%;
  height: 84rpx;  
  display: flex;
  align-items: center;
  border-bottom: 1px #d2d2d2 solid;
  background-color: #ffffff;
}
.cutListLeft{
  width:50%;
  height: 84rpx;
  text-indent:30rpx;
  display: flex;
  align-items: center;
}
.iconarrow{
  width:18rpx; 
  height:32rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
}
.powerBtn{
  width:50%;
  height: 84rpx;
  display: flex;
  align-items: center;
  justify-content:flex-end;
}





.listBox{
  width:100%;
  height: 110rpx;
  display: flex;
}
.listLeft{
  width:50%;
  height: 110rpx;
  display: flex;
  flex-direction: column;
  justify-content:center;
  border-bottom: 1rpx #d2d2d2 solid;
  text-indent:30rpx;
}
.listTitle{padding: 2rpx 0rpx;}
.listMoney{padding-right:30rpx;}
.listRight{
  width:50%;
  height: 110rpx;
  display: flex;
  flex-direction: row-reverse;
  align-items: center;
  border-bottom: 1rpx #d2d2d2 solid;  
}


/*隐藏元素*/
.hide{
    display:none;    
}
