
/* @import "../../images/common.wxss"; */

.color333{color: #333333}
.allBox{
  width: 100%;
  color: #333333;
  margin-top: 20rpx;
}
.cutListBox{
  width: 95%;
  height: 84rpx;  
  flex-grow:0;
  display: flex;
  align-items: center;
  border-bottom: 1px #d2d2d2 solid;
  background-color: #ffffff;
  text-indent: 20rpx;  
  font-size: 30rpx;  
}
.blueBox{
  width: 95%;
  height: 114rpx;
  background-color: #34D0DD;
  color: #fff;
  margin-top: 40rpx;
}
.startBox{
  height: 100%;
  display: flex;
  flex-grow:1;
  border-right: 1rpx solid #fff;
}
.endBox{
  height: 100%;
  display: flex;
  flex-grow:1;
  border-left: 1rpx solid #fff;
}
    
.canvasBox{
  width: 95%;
  height: 200px;/*无图表*/
  background-color: #fff; 
}

.canvas {
    width: 100%;
    height: 200px;
    margin-left: -10rpx;
}
.canvasTile{
  width: 95%;
  height: 114rpx;
   margin-top: 40rpx;
   font-size: 32rpx;
   background-color: #fff;
}

/*内部元素 单行，横排，上下左右居中*/
.centerA{
  display: flex;
  justify-content: center;
  align-items: center;
}
/*内部元素 单行，横排，上下左右居中*/
.centerB{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content:center;
}

.font32{
  font-size: 32rpx;
}
.font30{
  font-size: 30rpx;
}
.margin5{
  margin-right: 5rpx;
}





