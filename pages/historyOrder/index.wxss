.allBox{
  width: 100%;
  height: 100vh;
  display: flex;
  flex-direction: column;
}
    
.canvasBox{
  width: 100%;
  /*height: 250px; 有图表*/
  height: 50px;/*无图表*/


    background-color: #fff;
    font-size: 35rpx;
    color: #333;
    font-family: Helvetica,Arial,"Helvetica Neue","Droid Sans","Microsoft YaHei",sans-serif;
}
.canvas {
    width: 100%;
    height: 200px;
}

/*表格title*/
.titleBox{
  width:100%;
  height: 96rpx;
  border-top: 1rpx #d2d2d2 solid;
  border-bottom: 1rpx #d2d2d2 solid;
  background-color: #f2f2f2;
  display: flex;
}
/*内部元素 单行，横排，上下左右居中*/
.centerA{
  display: flex;
  justify-content: center;
  align-items: center;
}
/*列表盒子*/
.listBox{
  width:100%;
  height: auto;
  overflow: scroll;
  display: flex;
  flex-direction: column;
}


.topBox{
  width:100%;
  height: 84rpx;
  position:fixed; 
  top: 0px;
  z-index: 2;
  border-top: 1rpx #d2d2d2 solid;
  border-bottom: 1rpx #d2d2d2 solid;
  background-color: #f2f2f2;
}

/*内部元素 单行，横排，上下左右居中*/
.centerB{
  display: flex;
  flex-direction: row;
  
}

.font28{
  font-size: 26rpx;
}
.titleTxtA{
  width:10%;
  height: 96rpx;  
}
.titleTxtB{
  width:39%;
  height: 96rpx;  
}
.titleTxtC{
  width:20%;
  height: 96rpx;  
}
.titleTxtD{
  width:16%;
  height: 96rpx;  
}
.titleTxtE{
  width:15%;
  height: 96rpx;  
}
.btnBox{
  width:360rpx;
  height:74rpx;
  border:2rpx #3DCAE6 solid;
  border-radius: 39rpx;
  margin-bottom: 35rpx;
  overflow: hidden;
}
.btnLeft{
  width:183rpx;
  height: 66rpx;
  background-color: #3DCAE6;
}
.btnRight{
  width:180rpx;
  height: 66rpx;
  background-color: #fff;
}
.cutLine{
  margin-top: 20rpx;
  width:100%;
  height:24rpx;
  background-color: #f2f2f2;
  border-top: 1rpx #d2d2d2 solid;
  border-bottom: 1rpx #d2d2d2 solid;
}
.scrollBox{
  padding-top: 84rpx;
}
.cutListBox{
  width: 100%;
  height: 84rpx;  
  display: flex;
  align-items: center;
  border-bottom: 1px #d2d2d2 solid;
  background-color: #ffffff;
}
.cutListLeft{
  width:50%;
  height: 84rpx;
  text-indent:30rpx;
  display: flex;
  align-items: center;
}
.iconarrow{
  width:18rpx; 
  height:32rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
}
.powerBtn{
  width:50%;
  height: 84rpx;
  display: flex;
  align-items: center;
  justify-content:flex-end;
}






.listLeft{
  width:50%;
  height: 110rpx;
  display: flex;
  flex-direction: column;
  justify-content:center;
  border-bottom: 1rpx #d2d2d2 solid;
  text-indent:30rpx;
}
.listTitle{padding: 2rpx 0rpx;}
.listMoney{padding-right:30rpx;}
.listRight{
  width:50%;
  height: 110rpx;
  display: flex;
  flex-direction: row-reverse;
  align-items: center;
  border-bottom: 1rpx #d2d2d2 solid;  
}
