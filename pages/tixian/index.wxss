/**index.wxss**/
.cutLine{
  width:100%;
  height:24rpx;
  background-color: #f2f2f2;
  border-top: 1rpx #d2d2d2 solid;
  border-bottom: 1rpx #d2d2d2 solid;
}
.cutBox{
  width: 100%;
  height: 84rpx;  
  display: flex;
  flex-direction: row;/*横排*/
  justify-content:center;
  align-items: center;
  border-bottom: 1px #d2d2d2 solid;
  background-color: #ffffff;
}
.cutBoxleft{
    width:22%;
    height: 84rpx;
    display: flex;
    flex-direction: row;/*横排*/
    align-items: center;
    justify-content:space-between;
}
.cutBoxleftB{
    width:28%;
    height: 84rpx;
    display: flex;
    flex-direction: row;/*横排*/
    align-items: center;
    justify-content:space-between;
}
.cutBoxright{
    width:60%;
    height: 84rpx;
    display: flex;
    flex-direction: row;/*横排*/
    align-items: center;
}
.inputcss{
    width: 100%;
    border:1px #3DCAE6 solid; 
    height: 54rpx; 
    border-radius: 2px; 
    font-size: 26rpx;
}
.tishi{
  width:100%;
  height: 48rpx;
  display: flex;
  flex-direction: row;/*横排*/
  align-items: center;
  text-indent: 30rpx;
  font-size: 24rpx;
  color: #9d9d9d;
  background-color: #fff;
}
.tishiB{
  width:100%;
  height: 84rpx;
  margin-top: 64rpx;
  display: flex;
  flex-direction: row;/*横排*/
  align-items: center;
  justify-content:center;
  text-indent: 30rpx;
  font-size: 36rpx;
  background-color: #fff;
}
.tixianBox{
    width:100%;
    height: 84rpx;
    display: flex;
    flex-direction: row;/*横排*/
    align-items: center;
    justify-content:center;
}
.tixianBtn{
    width:80%;
    height: 84rpx;
    background-color: #3DCAE6;
    border-radius: 4rpx;
    color: #fff;
    display: flex;
    flex-direction: row;/*横排*/
    align-items: center;
    justify-content:center;
    font-weight: bold;
    font-size: 36rpx;
}
.tixianTxt{
    width:80%;
    height: 84rpx;
    color: #9d9d9d;
    display: flex;
    flex-direction: column;
    font-size: 24rpx;
}
.tixianTxtPadding{
    padding-top: 5rpx;
    padding-bottom:5rpx;
}